/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notsel);
    Or(a=a, b=sel, out=c1);
    Or(a=b, b=notsel, out=c2);
    And(a=c1, b=c2, out=out);
}